<DashboardResources />

<Router AppAssembly="@typeof(FullLayout).Assembly">
    <Found Context="routeData">
        <RouteView RouteData="@routeData" DefaultLayout="@typeof(FullLayout)" />
    </Found>
    <NotFound>
        <LayoutView Layout="@typeof(FullLayout)">
            <p>Sorry, there's nothing at this address.</p>
        </LayoutView>
    </NotFound>
</Router>
